<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gsein.blog.articlemanager.mapper.ArticleTypeMapper">
    <resultMap id="BaseResultMap" type="cn.gsein.blog.articlemanager.entity.ArticleType">
        <id column="article_type_id" jdbcType="VARCHAR" property="articleTypeId"/>
        <result column="type" jdbcType="VARCHAR" property="type"/>
    </resultMap>
    <select id="queryAll" resultMap="BaseResultMap">
        select article_type_id, type from article_type
    </select>
    <select id="queryByPrimaryKey" resultMap="BaseResultMap">
        select * from article_type where article_type_id = #{id}
    </select>
</mapper>